{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "2.0",
  "author": "YUKI \"Piro\" Hiroshi",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "bookmarks",
    "browserSettings",
    "storage"
  ],
  "background": {
    "page": "/background/background.html"
  },
  "icons": {
    "16": "/resources/Bookmark.svg#default",
    "20": "/resources/Bookmark.svg#default",
    "24": "/resources/Bookmark.svg#default",
    "32": "/resources/Bookmark.svg#default"
  },
  "options_ui": {
    "page": "/options/options.html"
  },
  "web_accessible_resources": [
    "resources/*",
    "panel/*"
  ],
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "tst-bookmarks-subpanel@piro.sakura.ne.jp",
      "strict_min_version": "67.0"
    }
  }
}
